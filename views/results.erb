<table class='table table-hover table-striped'>
  <% @attendees.each do |key,value|%>
    <tr>
      <th><%= @attendees[key]['name'] %></th>
      <td>Aanwezig:</td>
      <td><%=@attendees[key]['attending'] %></td>
      <td>Met partner</td>
      <td><%=@attendees[key]['partner'] %></td>
    </tr>
<% end %>
</table>
